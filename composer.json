{
    "name": "ogrosko/dw-content-protector",
    "type": "typo3-cms-extension",
    "description": "Protects flux content elements to be created/moved/copied to restricted column.",
    "version": "0.0.2",
    "license": "LGPL-3.0+",
    "keywords": [
        "TYPO3",
        "CMS",
        "Flux",
        "content"
    ],
    "require": {
        "typo3/cms-core": ">=8.7.0,<9.0.0",
        "fluidtypo3/flux": ">=8.0.0"
    },
    "autoload": {
        "psr-4": {
            "Digitalwerk\\DwContentProtector\\": "Classes"
        }
    },
    "replace": {
        "dw_content_protector": "self.version",
        "typo3-ter/dw-content-protector": "self.version"
    },
    "authors": [
        {
            "name": "Ondrej Grosko",
            "email": "ondrej@digitalwerk.agency"
        }
    ]
}
